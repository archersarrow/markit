{
  "name": "markdown-preview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o build",
    "start": "next start",
    "electron": "electron .",
    "electron-dev": "concurrently \"BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/main.js"
  },
  "main": "./electron/main.js",
  "dependencies": {
    "codemirror": "^5.58.2",
    "concurrently": "^3.5.0",
    "electron-store": "^6.0.1",
    "fibers": "^5.0.0",
    "next": "9.5.5",
    "node-sass": "^4.14.1",
    "react": "17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "17.0.1",
    "react-markdown": "^5.0.2",
    "react-syntax-highlighter": "^15.2.1",
    "remark-gfm": "^1.0.0",
    "sass": "^1.27.0",
    "wait-on": "^2.0.2"
  },
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^19.26.3",
    "electron-is-dev": "^0.3.0"
  }
}